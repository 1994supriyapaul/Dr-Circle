<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="2dp"
    android:id="@+id/puja_lay"
    android:background="@color/white">
    <RelativeLayout
        android:background="@drawable/style_box_background"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints">
        <RelativeLayout
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="0dp"
            android:padding="1dp"
            android:background="@drawable/doctor_back"
            android:id="@+id/os_images1"
            android:layout_width="60dp"
            android:layout_height="60dp">
            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/os_images"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:scaleType="centerCrop"
                android:src="@drawable/dr_img"
                android:layout_centerInParent="true"
                />
        </RelativeLayout>
        <LinearLayout
            android:id="@+id/ll"
            android:layout_toEndOf="@+id/os_images1"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/dr_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="30dp"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="0dp"
                android:layout_marginEnd="84dp"
                android:layout_marginStart="8dp"
                android:fontFamily="@font/proxima_nova_bold"
                android:text="Dr. Supriya Paul"
                android:textAlignment="textStart"
                android:textAllCaps="false"
                android:textColor="@color/colorName"
                android:textSize="16dp" />
            <RelativeLayout
                android:layout_marginBottom="0dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="4dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/call_ic"
                    android:src="@drawable/call_icon"
                    android:layout_width="20dp"
                    android:layout_height="20dp"/>
                <TextView
                    android:layout_toEndOf="@+id/call_ic"
                    android:id="@+id/call_num"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="0dp"
                    android:layout_marginBottom="0dp"
                    android:layout_marginEnd="4dp"
                    android:layout_marginStart="8dp"
                    android:fontFamily="@font/proxima_nova_bold"
                    android:text=""
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textColor="@color/colorPhone"
                    android:textSize="16dp" /> <TextView
                android:layout_toEndOf="@+id/call_num"
                android:id="@+id/call_num2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:layout_marginBottom="0dp"
                android:layout_marginEnd="4dp"
                android:layout_marginStart="0dp"
                android:fontFamily="@font/proxima_nova_bold"
                android:text=""
                android:textAlignment="textStart"
                android:textAllCaps="false"
                android:textColor="@color/colorPhone"
                android:textSize="16dp" />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_below="@+id/ll"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/location1"
                    android:layout_width="60dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:layout_marginBottom="0dp"
                    android:layout_marginEnd="4dp"
                    android:layout_marginStart="8dp"
                    android:fontFamily="@font/proxima_nova_bold"
                    android:text="Location"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textColor="@color/colorCatText"
                    android:textSize="14dp" />
                <TextView
                    android:id="@+id/place_id"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="4dp"
                    android:layout_marginBottom="0dp"
                    android:layout_marginEnd="4dp"
                    android:layout_marginStart="0dp"
                    android:fontFamily="@font/proxima_nova_regular"
                    android:text="67867"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textColor="@color/colorCatText"
                    android:textSize="14dp" />
            </LinearLayout>


            <RelativeLayout
                android:id="@+id/button_doctor"
                android:layout_width="80dp"
                android:layout_height="30dp"
                android:layout_marginBottom="8dp"
                android:layout_marginEnd="4dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/video_button"
                >
                <TextView
                    android:layout_centerInParent="true"
                    android:fontFamily="@font/proxima_nova_regular"
                    android:text="View Doctor"
                    android:textAllCaps="false"
                    android:textColor="#fff"
                    android:textSize="12sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </RelativeLayout>

        </LinearLayout>


        <RelativeLayout
            android:layout_alignParentEnd="true"
            android:id="@+id/button_login"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:layout_marginBottom="0dp"
            android:layout_marginEnd="4dp"
            android:layout_marginStart="0dp"
            android:layout_marginTop="4dp"
            android:background="@drawable/video_button"
            >
            <TextView
                android:layout_centerInParent="true"
                android:fontFamily="@font/proxima_nova_regular"
                android:text="View Video"
                android:textAllCaps="false"
                android:textColor="#fff"
                android:textSize="12sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>